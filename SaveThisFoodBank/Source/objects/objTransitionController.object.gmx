            <kind>1</kind>
            <string>///Initialize Data
x = view_xview[0] + view_wview[0] / 2;
y = view_yview[0] + view_hview[0] / 2;
setOkToMove(false);
setOkToPause(false);
alpha = 1;
newspaper_alpha = 0;
ok_to_fade = false;
ok_to_draw_prompt = false;

alarm[0] = room_speed * 3; //set ok_to_draw_prompt

var state = getState();

/*
    states
    0: pressed start
    1: entered collection area
    2: left food bank (end of first day)
    3: entered collection area
    4: left food bank (met first quota?)
    5: entered collection area
    6: left food bank (if not fired, crisis. If fired, end of game, bad ending)
    7: entered collection area
    8: End of game, good ending
*/

prompt = "Press ENTER to continue";
//what newspaper will we show?
switch(state){
    case 0:
        //opening newspaper
        newspaper = sprNewspaperStart;
        prompt = "Press ENTER to start";
        break;
    case 2:
        //New challenger approaching!
        newspaper = sprNewspaperEndDayOne;
        break;
    case 4:
        var met_first = getMetFirstQuota();
        if(met_first){
            //A hero!
            newspaper = sprNewspaperSuccess;
        }else{
            //A hero?
            newspaper = sprNewspaperFail;
        }
        break;
    case 6:
        //crisis!
        newspaper = sprNewspaperCrisis;
        if(!getMetFirstQuota() &amp;&amp; !getMetSecondQuota()){
            prompt = "Press ENTER to end";
        }
        break;
    case 8:
        //end of game
        newspaper = sprNewspaperWin;
        prompt = "Press ENTER to end";
        break;
    default:
        instance_destroy();
        break;
}


</string>
