            <string>///Move characters based on stage

var intro_stage = getIntroStage();

switch(intro_stage){
    case 0:
        //spawn player, move them a little to the right
        var destination = 288;
        if(instance_number(objPlayer) == 0){
            instance_create(160, 448, objPlayer);
            with(objPlayer){
                sprite_index = sprPlayerIdleRight;
                image_speed = 0;
            }
        }
        if(alarm[0] == -1){
            //alarm[0] sets ok_to_move_character
            alarm[0] = room_speed * 1;
        }
        if(ok_to_move_character){
            with(objPlayer){
                sprite_index = sprPlayerWalkRight;
                image_speed = CUTSCENE_WALKING_IMAGE_SPEED;
            }
            moveCharacter(1, objPlayer, destination);
        }
        if(getDoneMovingCharacters()){
            with(objBossNPC){
                sprite_index = sprBossLeft;
                image_speed = 0;
            }
            with(objPlayer){
                sprite_index = sprPlayerIdleRight;
                image_speed = 0;
            }
        }
        break;
    case 1:
        //move boss to left door, despawn
        var destination = 160;
        if(alarm[0] == -1){
            alarm[0] = room_speed * 1;
        }
        if(ok_to_move_character){
            with(objBossNPC){
                sprite_index = sprBossWalkingLeft;
                image_speed = CUTSCENE_WALKING_IMAGE_SPEED;
            }
            moveCharacter(-1, objBossNPC, destination);
        }
        if(getDoneMovingCharacters()){
            instance_destroy(objBossNPC);
        }
        break;
    case 2:
        //Resume gameplay routine
        setDoneMovingCharacters(true);
        break;
}

</string>
